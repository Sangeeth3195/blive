<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_below="@+id/top_area"
    android:layout_marginTop="50dp"
    android:layout_marginBottom="54dp"
    android:orientation="vertical"
    android:weightSum="6">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.2"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="3">

        <FrameLayout
            android:id="@+id/first_guestgroup9"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="end|center"
            android:layout_weight="1">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/second_guestgroup9"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="end"
            android:layout_weight="1">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/third_guestgroup9"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="end"
            android:layout_weight="1">

        </FrameLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="2dp"
        android:layout_weight="1.2"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="3">

        <FrameLayout
            android:id="@+id/eighth_guestgroup9"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="end|center"
            android:layout_weight="1"
            tools:ignore="NestedWeights">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/broadcaster_group9"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="end"
            android:layout_weight="1" />

        <FrameLayout
            android:id="@+id/fourth_guestgroup9"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="end"
            android:layout_weight="1">

        </FrameLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="2dp"
        android:layout_weight="1.2"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="3">

        <FrameLayout
            android:id="@+id/seventh_guestgroup9"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="end|center"
            android:layout_weight="1"
            tools:ignore="NestedWeights">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/sixth_guestgroup9"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="end"
            android:layout_weight="1">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/fifth_guestgroup9"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="end"
            android:layout_weight="1">

        </FrameLayout>

    </LinearLayout>

</LinearLayout>